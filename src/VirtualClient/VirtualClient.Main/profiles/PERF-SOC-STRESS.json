{
    "Description": "SOC Stress Workload",
    "Parameters": {
        "Host":"HostIP",
        "UserName":"username",
        "Password":"password",
        "DisableSOCPCIe": false,
        "DisableSOCDRAM": false,
        "DisableSOCPowerVirus": false,
        "FPGAStressVerbose": true,
        "FPGAFactoryTesterTimeout": 300,
        "SysbenchTimeout": 300
    },
    "Actions": [
        {
            "Type": "SOCStressFPGAFactoryTesterExecutor",
            "Parameters": {
                "Scenario": "ApplyFPGAFactoryTesterStress",
                "Host": "$.Parameters.Host",
                "UserName": "$.Parameters.UserName",
                "Password": "$.Parameters.Password",
                "DisableSOCPCIe": "$.Parameters.DisableSOCPCIe",
                "DisableSOCDRAM": "$.Parameters.DisableSOCDRAM",
                "DisableSOCPowerVirus": "$.Parameters.DisableSOCPowerVirus",
                "FPGAStressVerbose":  "$.Parameters.FPGAStressVerbose",
                "FPGAFactoryTesterTimeout": "$.Parameters.FPGAFactoryTesterTimeout"
            }
        },
        {
            "Type": "SOCStressSysbenchExecutor",
            "Parameters": {
                "Scenario": "ApplySysbenchStress",
                "Host": "$.Parameters.Host",
                "UserName": "$.Parameters.UserName",
                "Password": "$.Parameters.Password",
                "SysbenchMemoryCommandLine": "--test=memory --memory-block-size=1M --memory-total-size=200T --num-threads=8 --max-time=[sysbenchtimeout] run",
                "SysbenchCPUCommandLine": "--test=cpu --num-threads=8 --cpu-max-prime=2000000 --max-time=[sysbenchtimeout] run",
                "SysbenchTimeout": "$.Parameters.SysbenchTimeout"
            }
        }
    ]
}